{
	"presets": [
		["@babel/preset-react", {
            "runtime": "automatic"
        }],
		"@babel/preset-env",
		"@babel/preset-typescript"
	],
	"plugins": [
		"@babel/plugin-transform-runtime"
	],
	"env": {
		"dev": {
			"plugins": [
				["i18next-extract", {
					"locales": ["ru", "en"],
					"keyAsDefaultValue": true
				}],
				["./config/babel/plugins/removeProps.ts", {
					"props": ["data-testid"]
				}]
			]
		}
	}
}
